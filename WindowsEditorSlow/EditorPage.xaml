<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:windowsEditorSlow="using:WindowsEditorSlow"
             x:Class="WindowsEditorSlow.EditorPage"
             Title="EditorPage">
    <VerticalStackLayout>
        <windowsEditorSlow:RichEditor
            Text="{Binding LongText}"
            WidthRequest="600"
            HeightRequest="400"
            IsEnabled="false" />
    </VerticalStackLayout>
</ContentPage>